{
  "name": "prismweave-website",
  "version": "1.0.0",
  "description": "PrismWeave Website with Bookmarklet Generator",
  "private": true,
  "scripts": {
    "build": "node scripts/build-bookmarklet.js",
    "build:prod": "cross-env NODE_ENV=production node scripts/build-bookmarklet.js",
    "clean": "node scripts/cleanup-bookmarklet.js",
    "dev": "npm run build && node scripts/dev-server.mjs --port 3000",
    "dev:python": "npm run build && python -m http.server 3000",
    "serve": "python -m http.server 8080"
  },
  "keywords": [
    "bookmarklet",
    "content-capture",
    "web-tool"
  ],
  "author": "PrismWeave Team",
  "license": "MIT",
  "devDependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/api-logs": "^0.208.0",
    "@opentelemetry/auto-instrumentations-node": "^0.67.3",
    "@opentelemetry/exporter-logs-otlp-http": "^0.208.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.208.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.208.0",
    "@opentelemetry/resources": "^1.9.0",
    "@opentelemetry/sdk-logs": "^0.208.0",
    "@opentelemetry/sdk-metrics": "^1.9.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.25.5",
    "typescript": "^5.8.3"
  }
}
