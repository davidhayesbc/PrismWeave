{
  "name": "prismweave-markdown-dev-tools",
  "version": "1.0.0",
  "description": "Development tools for testing PrismWeave markdown conversion",
  "type": "module",
  "scripts": {
    "test-url": "node --loader ts-node/esm simple-url-test.js",
    "test-url-ts": "node --loader ts-node/esm test-url-conversion.ts",
    "test-file": "node --loader ts-node/esm test-file-conversion.ts",
    "compare": "node --loader ts-node/esm compare-outputs.ts",
    "build": "tsc --build",
    "clean": "rm -rf node_modules package-lock.json dist"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0-rc.12",
    "diff": "^5.2.0",
    "fs-extra": "^11.2.0",
    "jsdom": "^24.1.3",
    "node-fetch": "^3.3.2",
    "turndown": "^7.2.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/diff": "^5.2.1",
    "@types/fs-extra": "^11.0.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.0.0",
    "@types/yargs": "^17.0.32",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "overrides": {
    "node-domexception": "npm:domexception@^4.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
