{
  "name": "prismweave-markdown-dev-tools",
  "version": "1.0.0",
  "description": "Development tools for testing PrismWeave markdown conversion - uses exact browser extension code",
  "type": "module",
  "scripts": {
    "test-url": "tsx simple-url-test.ts",
    "test-url-direct": "tsx simple-url-test-tsx.ts",
    "build": "tsc --build",
    "clean": "rm -rf node_modules package-lock.json dist"
  },
  "dependencies": {
