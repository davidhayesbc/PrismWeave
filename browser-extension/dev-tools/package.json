    "cheerio": "^1.0.0-rc.12",
    "fs-extra": "^11.2.0",
    "jsdom": "^24.1.3",
    "node-fetch": "^3.3.2",
    "tsx": "^4.16.0",
    "turndown": "^7.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.33",
    "typescript": "^5.8.3"
  },
  "overrides": {
    "node-domexception": "npm:domexception@^4.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
