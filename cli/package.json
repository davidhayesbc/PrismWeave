{
  "name": "prismweave-cli",
  "version": "0.1.0",
  "description": "Command-line tool for capturing web pages as markdown and syncing to Git",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "prismweave": "./dist/index.js",
    "prismweave-capture": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "capture": "node dist/index.js",
    "clean": "rm -rf dist node_modules",
    "reinstall": "npm run clean && npm install && npm run build",
    "test": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js --coverage"
  },
  "keywords": [
    "cli",
    "markdown",
    "web-capture",
    "git",
    "documentation"
  ],
  "author": "PrismWeave Team",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.0.0",
    "puppeteer": "^22.0.0",
    "turndown": "^7.2.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/turndown": "^5.0.5",
    "@types/jest": "^29.5.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "jsdom": "^24.0.0",
    "@types/jsdom": "^21.1.0",
    "typescript": "^5.8.3"
  }
}
